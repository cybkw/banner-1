<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.to.aboomy.bannersample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.RecyclerActivity" />
        <activity android:name=".activity.FragmentViewPagerActivity" />
        <activity android:name=".activity.MultBannerActivity" />
        <activity android:name=".activity.AnimActivity" />
        <activity android:name=".activity.Indicator2Activity" />
        <activity android:name=".activity.RevealBannerActivity" />
        <activity android:name=".activity.VideoActivity" />
    </application>

</manifest>